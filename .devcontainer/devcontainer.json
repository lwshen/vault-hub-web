{
  "name": "Development Container",
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
  "features": {
    "ghcr.io/wandb/catnip/feature:1": {},
    "ghcr.io/devcontainers/features/node:1": {
      "version": "24"
    }
  },
  "forwardPorts": [
    5173,
    6369
  ],
  "portsAttributes": {
    "5173": {
      "label": "VaultHub Web",
      "onAutoForward": "notify"
    },
    "6369": {
      "label": "Catnip",
      "onAutoForward": "notify"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "vue.volar"
      ]
    }
  }
}
